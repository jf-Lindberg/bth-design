{% include 'incl/header.twig' %}

<body>
	{% include 'incl/nav.twig' %}
	<div class="main" role="main">
		{{ content }}
		{% for page in pages("analysis")|reverse if not page.hidden %}
			
			<a href="{{ page.url }}">
			<div>
				<h3>
					{{ page.title }}
				</h3>
				<p class="excerpt">{{ page.description }}</p>
			</div>
			</a>
		{% endfor %}
	</div>

	{% include 'incl/footer.twig' %}
</body></html>
